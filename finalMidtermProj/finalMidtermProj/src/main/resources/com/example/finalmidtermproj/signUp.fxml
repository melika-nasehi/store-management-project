<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchor" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finalmidtermproj.signUp">
   <children>
      <Label layoutX="255.0" layoutY="52.0" text="Sign up" textFill="#8fb814">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="133.0" layoutY="119.0" text="Username :" />
      <Label layoutX="135.0" layoutY="183.0" text="Password :" />
      <TextField fx:id="signUpUsername" layoutX="234.0" layoutY="115.0" promptText="username" />
      <TextField fx:id="signUpPassword" layoutX="234.0" layoutY="179.0" promptText="password" />
      <Button defaultButton="true" layoutX="258.0" layoutY="263.0" mnemonicParsing="false" onAction="#signUp" text="Confirm" />
   </children>
</AnchorPane>
